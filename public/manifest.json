{
  "manifest_version": 2,

  "name": "Eyes Above",
  "description": "A Chrome extension that visualizes and sonifies the artificial satellites orbiting above the user.",
  "version": "0.0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "geolocation",
    "storage"
  ],
  "icons": { "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" }
}